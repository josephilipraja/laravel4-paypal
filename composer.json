{
    "name": "josephilipraja/laravel4-paypal",
    "description": "laravel4-paypal is simple package help you process direct credit card payments, stored credit card payments and PayPal account payments with your L4 projects using paypal REST API SDK.",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Jose Philip Raja",
            "email": "mail@josephilipraja.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "~4.0",
        "ext-curl": "*",
        "ext-json": "*",
        "paypal/rest-api-sdk-php": "1.3.0"
    },
    "require-dev":{
        "phpspec/phpspec": "~2.0"
    },
    "autoload": {
        "psr-0": {
            "josephilipraja\\laravel4-paypal": "src/"
        }
    },
    "minimum-stability": "dev"
}
